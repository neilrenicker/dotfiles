# Unix
alias ping="ping -o"
alias ls="ls -p"
alias cp="cp -i"
alias mv="mv -i"
alias c="clear"

# git
alias cleanup="git remote prune origin && git branch --merged | grep -v \* | xargs git branch -D"

# Cake
alias cs="cake server"

# Grunt 
alias gt="grunt"
alias gw="grunt watch"

# Other
#
# Simple HTTP serve. cd into directory with an index.html file and type `serve`
alias serve='python -m SimpleHTTPServer 8000'
